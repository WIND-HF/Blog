<ol>
    <div id="playlist">
    <% @playlist.each do |m| %>
        <li id=<%= m.id %>>
            <p onclick="ajaxGetJSON('<%= switch_song_path %>', <%= m.id %>)">
            <%= m.name %>
            </p>
        </li>
    <% end %>
    </div>
</ol>

<form enctype="multipart/form-data" name="upload" >
    <input name="song" type="file" id="file" /><br><br>
    <input type="button" value="Upload" id="Hey" onclick="ajaxUpload()" />
</form>
<script>
    $("#file").on("change", function() {LimitFileSize();});
    $("#Hey").hide();
</script>
